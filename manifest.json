{
  "name": "Chrome Horrible Subs change",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "content_scripts": [
    {
      "css": [
        "style.css"
      ],
      "js": [
        "third_party/jquery-3.3.1.min.js",
        "content.js"
      ],
      "matches": [
        "https://horriblesubs.info/release-schedule/"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "https://horriblesubs.info/*"
  ]
}